---
const { images, alt, urls, tag } = Astro.props;
import MasonryLayout from "./Masonry.astro";
---

<div id="masonry" style={{width: '100%', columnCount: 4, columnGap: '1em'}}>
  <div
    id="homepage"
    class="object-cover object-center"
    style="object-position: 40% 35%;"
    data-images={images}
    data-alt={alt}
    data-urls={urls}
  >
  </div>
   <slot />
</div>
<script src="../scripts/homePage.js"></script>
